<Window x:Class="wahlanalyseTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wahlanalyseTool"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Niklas wahl analyse tool" HorizontalAlignment="Center" Height="32" Margin="0,10,0,0" VerticalAlignment="Top" Width="400" HorizontalContentAlignment="Center" FontWeight="Bold" FontSize="16"/>
        <Button x:Name="btn_Start_programm" Content="Analyse Starten" HorizontalAlignment="Center" Height="59" Margin="0,348,0,0" VerticalAlignment="Top" Width="500" FontSize="16" Click="btn_Start_programm_Click"/>
        <ComboBox x:Name="Cbox_select_workload" HorizontalAlignment="Left" Height="209" Margin="493,60,0,0" VerticalAlignment="Top" Width="157">
            <ComboBoxItem Content="show all"/>
            <ComboBoxItem Content="datagrid"/>
        </ComboBox>
        <ComboBox x:Name="CBox_select_datei" HorizontalAlignment="Left" Height="209" Margin="150,60,0,0" VerticalAlignment="Top" Width="142"/>

    </Grid>
</Window>
